{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-trailing-spaces": "error",
    "no-unused-vars": "warn",
    "semi": ["error", "never"],
    "quotes": ["warn", "double"],
    "jsx-quotes": ["error", "prefer-double"],
    "comma-dangle": ["error", "always-multiline"],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0
      }
    ],
    "eol-last": ["error", "always"],
    "no-console": [
      "warn",
      {
        "allow": ["warn", "error"]
      }
    ]
  }
}
